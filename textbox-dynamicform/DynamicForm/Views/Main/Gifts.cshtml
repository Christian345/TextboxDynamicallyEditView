@model System.Collections.Generic.IEnumerable<DynamicForm.Models.Gift>

@{
    ViewBag.Title = "Gifts";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Gifts</h2>
<div id="editorRows">
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <span><strong>Item:</strong> </span>
                @item.Name
                <span><strong>Price:</strong> $</span>
                @item.Price
            </li>
        }
    </ul>
</div>